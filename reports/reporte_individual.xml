<odoo>

	<record id="hoja_carta" model="report.paperformat">
		<field name="name">Tamaño Carta</field>
		<field name="default" eval="True"/>
		<field name="format">Letter</field>
	    <field name="page_height">0</field>
		<field name="page_width">0</field>
		<field name="orientation">Portrait</field>
		<field name="margin_top">20</field>
		<field name="margin_bottom">15</field>
		<field name="margin_left">15</field>
		<field name="margin_right">15</field>
		<field name="header_line" eval="False"/>
		<field name="header_spacing">15</field>
		<field name="dpi">80</field>
	</record>

	<report
		id="reporte_individual"
		model="catax.catax"
		string="Reporte individual"
		report_type="qweb-pdf"
		name="catax.reporte_individual_template"
		print_report_name="('Reporte-'+ str(object.folio_report) +'-CATAX')"
		paperformat="catax.hoja_carta"
	/>

  <template id="catax.reporte_individual_template">
		<t t-call="web.html_container">
					
			 	<div class="header" style="border-bottom: 1px solid black;">
				 	<img style="width: 80px;position: absolute;margin-top: -5px; margin-left: 5%;" src="data:image/png;base64," />

					<h3 class="text-center">Reporte - <span t-field="docs.folio_report"/></h3>
				</div>
				<div class="article mt0 o_report_layout_standard">
					<div class="col-xs-12 col-md-12  col-12">
						<div class="col-xs-6 col-md-6  col-6">
							<p><b>Fecha de creación: </b> <span t-field="docs.create_date"/></p>
							<p><b>Categoria: </b> <span t-field="docs.categoria.display_name"/></p>
							<p><b>Subcategoria: </b> <span t-field="docs.categoria.display_name"/></p>
						</div>
						<div class="col-xs-6 col-md-6  col-6">
							<p><b>Medio de recepción: </b> <span t-field="docs.medio_reporte"/></p>
							<t t-if="docs.telefono != False or docs.telefono != '' ">
								<p><b>Teléfono: </b> <span t-esc="docs.telefono"  /></p>
							</t>
							<t t-if="docs.correo != False or docs.correo != ''">
								<p><b>
								Correo eléctronico: </b> <span t-esc="docs.correo"  /></p>
							</t>
						</div>
						<div class="col-xs-12 col-md-12  col-12">
						
							<p><b>Descripción del reporte : </b> <span t-field="docs.descripcion_reporte"/></p>
							<p><b>Observaciones : </b> <span t-field="docs.observaciones"/></p>
						</div>
					</div>
					

					<div class="col-xs-12 col-md-12  col-12">
						<t t-if="docs.report_cmas.numero_registro != False and docs.report_cmas.numero_registro != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Número registro: </b> <span t-esc="docs.report_cmas.numero_registro"  /></p></div>
						</t>

						<t t-if="docs.report_cmas.numero_medidor != False and docs.report_cmas.numero_medidor != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Número medidor: </b> <span t-esc="docs.report_cmas.numero_medidor"  /></p></div>
						</t>

						<t t-if="docs.report_cmas.tipo_material != False and docs.report_cmas.tipo_material != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Tipo de medidor: </b> <span t-esc="docs.report_cmas.tipo_material"  /></p></div>
						</t>

						<t t-if="docs.report_alumbrado.no_luminaria != False and docs.report_alumbrado.no_luminaria != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Número de luminaria: </b> <span t-esc="docs.report_alumbrado.no_luminaria"  /></p></div>
						</t>

						<t t-if="docs.nombre_padre != False and docs.nombre_padre != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Nombre del padre o tutor: </b> <span t-esc="docs.nombre_padre"  /></p></div>
						</t>

						<t t-if="docs.nombre_solicitante != False and docs.nombre_solicitante != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Nombre del solicitante: </b> <span t-esc="docs.nombre_solicitante"  /></p></div>
						</t>

						<t t-if="docs.direccion_instancia != False and docs.direccion_instancia != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Dirección de la escuela, instancia o dependencia: </b> <span t-esc="docs.direccion_instancia"  /></p></div>
						</t>

						<t t-if="docs.nombre_dependencia != False and docs.nombre_dependencia != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Dependencia o instancia desde donde se comunica: </b> <span t-esc="docs.nombre_dependencia"  /></p></div>
						</t>

						<t t-if="docs.personas_atrapadas != False and docs.personas_atrapadas != '' ">
							<t t-if="docs.personas_atrapadas == '0' ">
								<div class="col-xs-6 col-md-6  col-6"><p><b>Personas atrapadas: </b> No</p></div>
							</t>
							<t t-if="docs.personas_atrapadas == '1' ">
								<div class="col-xs-6 col-md-6  col-6"><p><b>Personas atrapadas: </b> Si</p></div>
							</t>
							
						</t>

						<t t-if="docs.tipo_tanque != False and docs.tipo_tanque != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Tipo de tanque: </b> <span t-esc="docs.tipo_tanque"  /></p></div>
						</t>

						<t t-if="docs.afectacion_vivienda != False and docs.afectacion_vivienda != '' ">
							<t t-if="docs.afectacion_vivienda == '0' ">
								<div class="col-xs-6 col-md-6  col-6"><p><b>Afectación en vivienda: </b> Al interior</p></div>
							</t>
							<t t-if="docs.afectacion_vivienda == '1' ">
								<div class="col-xs-6 col-md-6  col-6"><p><b>Afectación en vivienda:  </b> Al exterior</p></div>
							</t>
						</t>

						<t t-if="docs.nombre_completo != False and docs.nombre_completo != '' ">
							<div class="col-xs-6 col-md-6  col-6"><p><b>Nombre completo: </b> <span t-esc="docs.nombre_completo"  /></p></div>
						</t>
					
						<div class="col-xs-12 col-md-12  col-12">
							<h4 style="text-align:center">Ubicación</h4>
						</div>
						<div class="col-xs-6 col-md-6  col-6">
							<p><b>Colonia: </b> <span t-field="docs.colonia.display_name"/></p>
							<p><b>Calle: </b> <span t-field="docs.calle"/></p>
						</div>
						<div class="col-xs-6 col-md-6  col-6">
							<p><b>No. exterior : </b> <span t-field="docs.no_exterior"/></p>
							<p><b>No. Interior : </b> <span t-field="docs.no_interior"/></p>
							
						</div>
						<div class="col-xs-12 col-md-12  col-12">
							<p><b>Referencia : </b> <span t-field="docs.referencia"/></p>
						</div>
						
					</div>

					<div class="col-xs-12 col-md-12  col-12">
						<div class="col-xs-6 col-md-6  col-6">
							<img t-attf-src="data:image/png;base64,{{url_mapa}}" style="text-align:center;width: 90%;"/>
						</div>
						<div class="col-xs-6 col-md-6  col-6">
							<t t-if="docs.fotografiaReporte != False and docs.fotografiaReporte != '' and docs.fotografiaReporte != None ">
								<img t-attf-src="data:image/png;base64,{{docs.fotografiaReporte}}" style="text-align:center; width: 90%;"/>
							</t>
						</div>

					</div>

					
				</div>
				<div class="footer o_standard_footer" style="border-top: 1px solid black;">
					
				</div>
				
		</t>
	</template>
</odoo>
